<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.douyu.danmu.dao.DanmuDao">
    <insert id="insertMsg" parameterType="com.douyu.danmu.base.DanmuBase">
      INSERT INTO danmu values (#{msgID},#{userId},#{userName},#{userLevel},#{fansName},#{fansLevel},#{msg},#{msgColor},#{msgNoble},#{createTime})
    </insert>
</mapper>